{
  "name": "affine-workers-bun",
  "version": "1.0.0",
  "type": "module",
  "module": "src/index.js",
  "scripts": {
    "dev": "bun run --watch src/index.ts",
    "build": "bun build --compile --minify-whitespace --minify-syntax --target bun --outfile server ./src/index.ts",
    "lint": "prettier --check . && eslint .",
    "format": "prettier --write . && eslint . --fix"
  },
  "dependencies": {
    "@elysiajs/cors": "^1.1.1",
    "cheerio": "^1.0.0",
    "elysia": "^1.1.23",
    "elysia-helmet": "^2.0.0",
    "jsdom": "^25.0.1",
    "sharp": "^0.33.5",
    "tldts": "^6.1.56"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/jsdom": "^21.1.7",
    "eslint": "^9.13.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.2.1",
    "eslint-plugin-simple-import-sort": "^12.1.1",
    "prettier": "^3.3.3",
    "typescript-eslint": "^8.12.2"
  },
  "peerDependencies": {
    "typescript": "^5.0.0"
  },
  "trustedDependencies": [
    "sharp"
  ]
